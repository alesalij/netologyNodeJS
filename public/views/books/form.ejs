<form method="POST" enctype="multipart/form-data">
  <%   Object.keys(book.schema.obj).forEach((key,index)=>{
    if(index>=0&&index<Object.keys(book.schema.obj).length-1){
 %>
  <div class="row mb-3">
    <label class="col-sm-2 col-form-label"><%= key %>:</label>
    <div class="col-sm-10">
      <input
        type="form-text"
        class="form-control"
        id="input<%= key %>"
        name="<%= key %>"
        value="<%= book[key] %>"
      />
    </div>
  </div>
  <%} }) %> 
   <div class="row mb-3">
    <label class="col-sm-2 col-form-label">fileBook:</label>
    <div class="col-sm-10">
      <input type="file" class="form-control-file" id="inputfileBook"
      name="fileBook" value="<%= book["fileBook"] %>" />
    </div>
  </div>
  <button type="submit" class="btn btn-primary btn-lg btn-block">Save Changes</button>
</form>
